{
  "name": "neo-traverse",
  "version": "0.1.0",
  "description": "Traverse and transform objects by visiting every node on a recursive walk without `this`!",
  "main": "index.js",
  "repository": "git@github.com:babie/neo-traverse.git",
  "author": "babie <babie.wildheart@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "watch": "run-p watch:build watch:test",
    "watch:build": "tsc -p tsconfig.json --watch",
    "watch:test": "mocha --require espower-typescript/guess './**/*test.ts' --watch",
    "build": "tsc -p tsconfig.json",
    "test": "mocha --require espower-typescript/guess './**/*test.ts'",
    "lint": "tslint -p tsconfig.json"
  },
  "directories": {
    "test": "test/"
  },
  "devDependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/mocha": "^5.2.1",
    "@types/power-assert": "^1.5.0",
    "espower-typescript": "^8.1.3",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "power-assert": "^1.5.0",
    "prettier": "^1.13.4",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-config-prettier": "^1.13.0",
    "tslint-config-standard": "^7.0.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.1"
  }
}
